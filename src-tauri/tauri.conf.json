{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "height": 800,
        "minHeight": 600,
        "minWidth": 800,
        "title": "Harbor",
        "width": 1200
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "io.harbor.desktop",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/nicholasoxford/harbor/releases/latest/download/latest.json"
      ],
      "pubkey": "UPDATER_PUBKEY_PLACEHOLDER"
    }
  },
  "productName": "Harbor",
  "version": "1.0.0"
}